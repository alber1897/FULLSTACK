function cajero(np,cantidad){
    if(np=="1897"){
        console.log("Tu pin es correcto");
    
        if(cantidad>=200){
            console.log("Puedes sacar dinero");
        }
    
        else{
            console.log("No puedes sacar dinero");
        }
    
    }else{
        console.log("Tu pin es incorrecto");
    }
}
cajero(1897,150);


let rol="editor";

if(rol=="Administrador"){
    console.log("Bienvenido admin");
} else if(rol=="editor"){
    console.log("Bienvenido editor");
}
else{
    console.log("No tienes acceso");
}

let estacionActual="Otoño";

if(estacionActual=="Invierno"){
    console.log("Es la estacion del año, la siguiente estacion es Primavera");
}else if(estacionActual=="Primavera"){
    console.log("Es la estacion del año, la siguiente estacion es Verano");
}
else if(estacionActual=="Verano"){
    console.log("Es la estacion del año, la siguiente estacion es Otoño");
}
else if(estacionActual=="Otoño"){
    console.log("Es la estacion del año, la siguiente estacion es Invierno");
}
else{
    console.log("No has introducido una estacion del año");
}


function notas(n){
    if(n>=9 && n<=10){
        console.log("Tienes un sobresaliente");
    }else if(n>=7 && n<9){
        console.log("Tienes un notable");
    }
    else if(n>=5 && n<7){
        console.log("Es la estacion del año, la siguiente estacion es Otoño");
    }
    else if(n>0 && n<5){
        console.log("Estas suspenso");
    }
    else{
        console.log("No has introducido una nota valida");
    }
}
notas(3);

// let metodo=prompt("Mete metodo de pago");
let metodo=10;
switch(metodo){
    case "Tarjeta": console.log("Pagaste con tarjeta");
    break;
    
    case "Efectivo": console.log("Pagaste con efectivo");
    break;

    case "Bizum": console.log("Pagaste con bizum");
    break;

    default: "Pendiente de pago";
    break;
}

let num=11;
function multiploConmutador(num){
    switch(true){
        case num*num<100 : console.log(`El resultado de ${num} por si mismo es menor que 100`);
        break;
    
        case num*num<200 && num*num>100 : console.log(`El resultado de ${num}
         por si mismo es mayor que 100 y menor que 200`);
        break;
    
        default: console.log("No esta dentro de lo previsto");;
        break;
    }
}
multiploConmutador(4);

function multiploConSi(num){
    if(num*num<100){
        console.log(`El resultado de ${num} por si mismo es menor que 100`);
    }else if(num*num<200 && num*num>100){
        console.log(`El resultado de ${num} por si mismo
        es mayor que 100 y menor que 200`);
    }else{
        console.log("No esta dentro de lo previsto");
    }
}
multiploConSi(10);

let num2=parseInt(prompt("introduzca numero"));

switch(num2+10){
    case 9: console.log(`Es igual a 9`); 
    break;

    case 12: console.log(`Es igual a 12`); 
    break;

    case 15: console.log(`Es igual a 15`); 
    break;

    case 100: console.log(`Es igual a 100`); 
    break;

    default: console.log(`No esta en lo previstoo`);
    break;
}


// IRÁ EJECUTANDO EL CÓDIGO SIEMPRE QUE SE CUMPLE LA CONDICION

for (let i = 0; i <= 3; i++) {
    //i === ÍNDICE, SUELE EMPEZAR POR 0, PUES SE USA MUCHO PARA
    //RECORRER ARREGLOS
    //i<=3 SERÁ LA CONDICIÓN. SE EJECUTA MIENTRAS SE CUMPLE
    //i++ ACTUALIZADOR
    console.log(i);

}


// EJERCICIO, MOSTRAR DEL 100 AL 200 POR PANTALLA



for (let i = 100; i <= 200; i++) {
    console.log(`El numero ${i} está entre el 100 y el 200`);
} // :) 

// PODEMOS USAR LOS COMPARADORES YA VISTOS, ===,<=,>=,<,>!==

/*
!!!!!!!!!!!!!!!!
EJERCICIO5
MOSTRAR EN ORDEN DESCENDENTE DEL 100 HASTA EL 0, PERO
DE 5 EN 5
INCLUIDOS AMBOS


*/
for (let i = 100; i >= 0; i -= 5) {
    console.log("De 100 a 0 de 5 en 5:", i)
};

