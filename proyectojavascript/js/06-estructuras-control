30, 28 O 31 DÍAS
*/

// ejercicio17("febrero");

let ejercicio17 = function (nombre) {
    if (nombre == "enero" || nombre == "marzo" || nombre == "mayo") {
console.log("tiene 31 dias"); 
    }else if(nombre=="febrero"){

        console.log("tiene 28 dias")
    }else{console.log("tiene 30 dias")};
}
ejercicio17("febrero")






